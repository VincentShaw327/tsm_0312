{
  "entry": "app/client.js",
  "extraBabelPlugins": [
    "transform-decorators-legacy",
    ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }]
  ],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ]
    }
  },
  "ignoreMomentLocale": true,
  "theme": "./theme.js",
  "html": {
      "template": "./app/index.ejs"
    },
  "publicPath": "/",
  "disableDynamicImport": true,
  "hash": true
}
